@model List<Diplom2.Models.StudentModel>

@{
    ViewData["Title"] = "Список студентов";
}

<h2>Список студентов</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Имя</th>
            <th>Фамилия</th>
            <th>Отчество</th>
            <th>Возраст</th>
            <th>Группа</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Surname</td>
                <td>@student.MiddleName</td>
                <td>@student.Age</td>
                <td>@student.Group</td>
            </tr>
        }
    </tbody>
</table>
